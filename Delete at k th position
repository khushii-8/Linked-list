Node* deleteAtK(Node* head, int k) {
    if (head == NULL) return NULL;    // empty list

    if (k == 1) {   // deletion at head
        Node* temp = head;
        head = head->next;
        delete temp;
        return head;
    }

    Node* temp = head;
    for (int i = 1; temp != NULL && i < k - 1; i++) {
        temp = temp->next;
    }

    if (temp == NULL || temp->next == NULL) return head; // k out of range

    Node* toDelete = temp->next;
    temp->next = temp->next->next;
    delete toDelete;

    return head;
}
